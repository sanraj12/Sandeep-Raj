
public class SynchronizationTest {

	int count = 0;

	public static void process()
	{
	
	Runnable r1 = new Runnable() {

		@Override
		public void run() {
			for (int i = 0; i < 100; i++) {
				count++;
			}

		}
	};

	Runnable r2 = new Runnable() {
		public void run() {
			for (int i = 0; i < 100; i++) {
				count++;
			}
		}
	};
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Thread t1 = new Thread(r1);
	}

}



